[Unit]
Description=Fetch Menu Data from Database
After=syslog.target

[Service]
Type=simple
User=root
Group=root
WorkingDirectory=/opt/iiab/menus-service
Environment=FLASK_APP=/opt/iiab/menus-service/test.py
ExecStart=flask run --host=0.0.0.0
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
